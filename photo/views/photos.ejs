<!doctype html>
<html>
	<head>
		<meta  charset="utf-8" />
		<title><%= title %></title>
		<link  rel="stylesheet" href="/stylesheets/style.css" />
	</head>
	<body>
		<h1>图片列表</h1>
		<p>Express 图片应用</p>
		<div class="photos">
			<% photos.forEach(function(photo){ %>
					<div class="photo">
							<h2><%= photo.name %></h2>
 							<img src="<%= photo.path %>" />
					</div>
			<%	}) %>
		</div>
	<body/>
</html>